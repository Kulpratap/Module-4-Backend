<?php

/**
 * Implements hook_preprocess_node().
 */
function my_custom_module_preprocess_node(&$variables) {
  if ($variables['node']->bundle() === 'article') {
    $variables['#attached']['library'][] = 'my_custom_module/my_custom_library';

    $variables['#attached']['drupalSettings']['myCustomModule'] = [
      'someValue' => 'change_color',
    ];
  }
}
